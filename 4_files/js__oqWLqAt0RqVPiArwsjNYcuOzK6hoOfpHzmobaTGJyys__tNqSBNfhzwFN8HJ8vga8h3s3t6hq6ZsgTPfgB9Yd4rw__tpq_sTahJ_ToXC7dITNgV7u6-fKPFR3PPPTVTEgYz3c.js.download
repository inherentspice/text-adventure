(function ($) {
  /**
   * Scroll carousels with swipe.
   */
  Drupal.behaviors.bootstrap_carousels = {
    attach: function(context) {
      $('.carousel.slide .carousel-inner', context).swiperight(function() {
        var carousel = $(this).closest('.carousel.slide');
        carousel.carousel('prev');
      }).swipeleft(function() {
        var carousel = $(this).closest('.carousel.slide');
        carousel.carousel('next');
      });
    }
  };

  /**
   * Show Bootstrap tooltips.
   */
  Drupal.behaviors.bootstrap_tooltips = {
    attach: function(context) {
      $('[data-toggle="tooltip"]', context).tooltip();
    }
  };

  /**
   * Add classes for correct theming of test progress bars.
   */
  Drupal.behaviors.truityTestProgress = {
    attach: function(context) {
      var last_done = $('.webform-client-form .tf-section-wrap .tf-section-done + .tf-section-on', context).prev();
      last_done.addClass('tf-section-done-last');
      last_done.prev().addClass('tf-section-done-prev-to-last');

      var after_active = $('.webform-client-form .webform-component-markup .progress li.active ~ li', context);
      after_active.addClass('future');
    }
  };

  /**
   * Second pair of controls in the text carousel.
   */
  function slide_indicators(parent_carousel, to_index) {
    var wrapper = $('.carousel-indicators-wrapper', parent_carousel);
    var to_elem = $('ol.carousel-indicators.carousel-indicators-pics li[data-slide-to=' + to_index + ']', parent_carousel)
      .add('ol.carousel-indicators.carousel-indicators-pics li[data-slide-for=' + to_index + ']', parent_carousel);
    var scroll_to = to_elem.position().left + to_elem.outerWidth(true) / 2 - wrapper.width() / 2;
    wrapper.animate({
      scrollLeft: scroll_to
    }, 500);
  }

  function carousel_is_wide(carousel) {
    if (!carousel.hasClass('carousel-len-l')) {
      return false;
    }

    var elem = $('ol.carousel-indicators.carousel-indicators-pics li', carousel);
    var wrapper = $('.carousel-indicators-wrapper', carousel);
    return elem.outerWidth() * 3 <= wrapper.width() + 3;
  }

  Drupal.behaviors.truityTextCarousel = {
    attach: function(context) {
      var carousel = $('.doubleslide-carousel', context);
      carousel.on('slide.bs.carousel', function (e) {
        var target = $(e.relatedTarget).index();
        var parent_carousel = e.target;
        var max_element = $('ol.carousel-indicators.carousel-indicators-normal li', parent_carousel).length - 1;

        if ((target === 0 || target === max_element) && carousel_is_wide($(parent_carousel))) {
          $(parent_carousel).carousel(e.direction === 'left' ? 1 : max_element - 1);
          e.preventDefault();
          return;
        }

        slide_indicators(parent_carousel, target);
        $('ol.carousel-indicators.carousel-indicators-normal li', parent_carousel).removeClass('active-alt');
        $('ol.carousel-indicators.carousel-indicators-normal li[data-slide-to=' + target + ']', parent_carousel).addClass('active-alt');
      });

      $('.carousel-indicators-wrapper', carousel).perfectScrollbar();

      carousel.each(function(index, one_carousel) {
        var onec = $(one_carousel);
        if (carousel_is_wide(onec)) {
          onec.carousel(1);
        }
      });
    }
  };

  /**
   * "Choose personality type" dropdown.
   */
  Drupal.behaviors.truityTypeDropdown = {
    attach: function(context) {
      $('.personality-types-menu .type-select', context).click(function(event) {
        $(this).closest('.personality-types-menu').toggleClass('types-menu-expanded');
      });
    }
  };

  /**
   * Collapsible blocks in the sidebar.
   */
  Drupal.behaviors.truityBlocksCollapsible = {
    attach: function(context) {
      $('.block-collapsible .block-title', context).click(function(event) {
        $(this).closest('.block-collapsible').toggleClass('block-collapsed');
      });
    }
  };

  /**
   * Image tests option selection.
   */
  Drupal.behaviors.imageTestSelect = {
    applySelection: function (radio) {
      var parent = $(radio).closest('.form-item.webform-component-radios');
      var siblings = $('input[type=radio]', parent);
      var i = 0;
      for (; i < siblings.length; ++i) {
        if (siblings[i] == radio) {
          break;
        }
      }
      if (i == siblings.length) {
        return;
      }
      var images = parent.nextAll('.form-item.webform-component-markup').first().find('.visual-img img');
      images.removeClass('selected');
      $(images[i]).addClass('selected');
    },

    attach: function(context) {
      var app = this.applySelection;
      $('div.form-item.webform-component[class*="imgtest"] input[type=radio]', context).change(function(e) {
        app(this);
      }).each(function() {
        if (this.checked) {
          app(this);
        }
      });
    }
  };

  /**
   * Popup forms opening override.
   */
  Drupal.popup_forms_child = Drupal.popup_forms_child || {processed: false};

  Drupal.popup_forms_child.attach = function (context) {
    if (typeof Drupal.settings.popup_forms_child == 'undefined') {
      return;
    }

    var settings = Drupal.settings.popup_forms_child;
    Drupal.settings.popup_options = JSON.parse(decodeURIComponent(window.location.href.substr(window.location.href.indexOf('&options=') + 9)));
    Drupal.settings.popup_options.backurl = unescape(Drupal.settings.popup_options.backurl);

    var parent = jQuery(window.parent);
    var scroll = parent.scrollTop();
    var window_height = parent.height();

    //Drupal.settings.popup_options.close = Drupal.popup_forms_child.closeDialog;
    var modal = $('#popup-forms-modal');
    var content =  $('.modal-dialog', modal);
    var dialog_height = content.height() + 10;
    if(dialog_height < window_height) {
      scroll += Math.round((window_height - dialog_height) / 2);
    }
    modal.css('padding-top', '' + scroll + 'px');

    Drupal.settings.popup_options.dialogClass.split(' ').forEach(function (cls) {
      content.addClass(cls);
    });

    if (typeof Drupal.settings.popup_options.width !== 'undefined') {
      content.css('width', Drupal.settings.popup_options.width);
    }

    $('a', modal).click(function(event) {
      Drupal.popup_forms_child.closeDialog();
      event.preventDefault();
    });

    modal.modal();
    modal.on('hidden.bs.modal', function (e) {
      Drupal.popup_forms_child.closeDialog();
    });

    $.postMessage({type: 'finish_loading', index: settings.index}, Drupal.settings.popup_options.backurl, window.parent);
  };

  Drupal.behaviors.popup_forms_child = {
    attach: Drupal.popup_forms_child.attach
  };

  Drupal.popup_forms = Drupal.popup_forms || {dialogs: []};
  Drupal.popup_forms.create = function (index) {
    var self = this;
    var iframe = jQuery('<iframe style="z-index:10" frameborder="0" allowtransparency class="popup-forms-iframe" id="popup-forms-iframe-' + index + '" name="popup-forms-iframe' + index + '"' + (typeof(jQuery.browser) != 'undefined' && jQuery.browser.msie ? ' scrolling="yes"' : '') + '/>');
    jQuery('body').append(iframe);
    self.dialogs[index].iframe = jQuery('#popup-forms-iframe-' + index);
  };

  /**
   * Overview personality type quotes slider.
   */
  Drupal.behaviors.truityQuotesSlider = {
    attach: function(context) {
      $('.views-field-field-pp-quotes').once(function(event) {
        var content = $('h2', this).nextUntil();
        $(content).wrapAll('<div class="carousel-inner"></div>');
        $('.carousel-inner', this).wrap('<div class="text-carousel carousel slide"></div>');

        $('.text-carousel', this).addClass('text-carousel carousel slide')
              .attr('data-ride', 'carousel')
              .attr('data-interval', '10000')
              .attr('id', 'carousel-text-pp-quotes')
              .append('<div class="carousel-controls">' +
                  '<a class="carousel-arrow arrow-left" data-target="#carousel-text-pp-quotes" role="button" data-slide="prev"><span class="sr-only">Previous</span></a>' +
                  '<ol class="carousel-indicators carousel-indicators-normal"></ol>' +
                  '<a class="carousel-arrow arrow-right" data-target="#carousel-text-pp-quotes" role="button" data-slide="next"><span class="sr-only">Previous</span></a>' +
                '</div>');

        $('.carousel-inner', this).attr('role', 'listbox');

        $('.right-align', this).wrap('<div class="quote-source"></div>').removeClass('right-align');
        $('.quote-source', this).wrap('<div class="item"></div>');
        $('.item', this).first().addClass('active');
        var carouselField = this;
        $('.item', this).each(function(index, value) {
          $(value).prepend($(value).prev().wrap('<div class="quote-text"></div>').parent());
          $('.carousel-indicators', carouselField).append('<li data-target="#carousel-text-pp-quotes" data-slide-to="' + index + '"></li>');
        });

        $('.carousel-indicators li', this).first().addClass('active');
      });
    }
  };

  /**
   * Manage number split into four-digit groups: "1234 5678 1234 5678".
   * @param value
   *   The number.
   * @returns {[*,*]}
   */
  function formatCreditCard(value) {
    var v = value.replace(/\s+/g, '').replace(/[^0-9]/gi, '');
    var matches = v.match(/\d{4,19}/g);
    var match = matches && matches[0] || '';
    var parts = [];
    for (i = 0, len = match.length; i < len; i += 4) {
      parts.push(match.substring(i, i + 4));
    }
    if (parts.length) {
      return [matches[0], parts.join(' ')];
    }
    else {
      return [v, v];
    }
  }

  /**
   * Draw a text field with formatted credit card number.
   */
  Drupal.behaviors.creditCardFormat = {
    attach: function(context) {
      var card_item = $('.form-item-commerce-payment-payment-details-credit-card-number', context);
      card_item.each(function(index, element) {
        var main_field = $('input', element).hide();
        $(element).append('<input class="credit-card-formatted form-control form-text" type="text" placeholder="0000 0000 0000 0000" />');
        $('input.credit-card-formatted', element)
          .on('input', function(event) {
            var format = formatCreditCard(this.value);
            this.value = '';
            main_field.val(format[0]);
            this.value = format[1];
          })
          .val(formatCreditCard(main_field.val())[1]);
      });
    }
  };

  /**
   * Behaviour for additional "cancel coupon" link on the order review page.
   */
  Drupal.behaviors.bootstrap_close_coupon_ink = {
    attach: function(context) {
      // Show/hide coupon subform.
      $('.bootstrap-truity-commerce-coupons-link:not(.ajax-processed)', context).addClass('ajax-processed').click(function(event) {
        $(this).parents('form').find('a.truity-commerce-coupons-link').trigger('click');
        event.preventDefault();
      });
    }
  };

  /**
   * Highlight "pay now" button on the "order review" page.
   */
  Drupal.behaviors.bootstrap_highlight_pay_button = {
    attach: function(context) {
      // "Pay" button.
      var updatePayButtonStatus = function() {
        var $unfilledFields = $('#payment-details').find('input[type="text"]').filter(function() {
          return this.value === '';
        });
        if ($unfilledFields.size() === 0) {
          $('#commerce-checkout-form-review #edit-continue').addClass('highlight');
        }
        else {
          $('#commerce-checkout-form-review #edit-continue').removeClass('highlight');
        }
      };
      // Refresh button status on radio button selection.
      $('#edit-commerce-payment-payment-method input', context).once('bootstrap_highlight_pay_button', function() {
        // "once" is not a mistake.
        $(this).change(function() {
          updatePayButtonStatus();
        });
      });
      // Refresh button status on textfield actions.
      $('#commerce-checkout-form-review #payment-details input[type="text"]', context).on('change keyup', function() {
        updatePayButtonStatus();
      });
      // Do the same for the new ajax payment details.
      if ($(context).attr('id') === 'payment-details') {
        context.find('input[type="text"]').on('change keyup', function() {
          updatePayButtonStatus();
        });
        // Refresh after options switch.
        updatePayButtonStatus();
      }
      $('.checkout-continue', context).each(function () {
        updatePayButtonStatus();
      });
    }
  };

  /**
   * Implements a sticky header.
   */
  Drupal.behaviors.stickyHeader = {
    attach: function (context) {
      $('.page-node-view.node-type-personality-test .question-header', context)
        .once('sticky-header', function () {
          if ($(this).parents().filter('.webform-component[class*="imgtest"]').length) {
            return;
          }

          var $header = $(this),
            $form = $header.closest('form'),
            $last_row = $form.find('.webform-component-radios').last(),
            header_offset = 0,
            header_height = $header[0].getBoundingClientRect().height,
            window_width = $(window).width(),
            min_window_width = 999,
            logged_in = $('body').hasClass('logged-in'),
            // Placeholder is required to compensate for the height occupied
            // by the header when the header is fixed at the top of the window.
            $placeholder = {};

          /**
           * Implements header sticking and unsticking at the end of a table.
           */
          function stick() {
            // Only for authorized users, it makes sense to check for the
            // presence of the administrative menu.
            if (logged_in) {
              $('#admin-menu').once('sticky-header-admin-menu', function () {
                header_offset += $(this).height();
              });
            }

            // This calculation is required for the table header to properly adhere to the sticky page header.
            $('#navbar', '.sticky-navbar').once('sticky-header-navbar', function () {
