(function($){Drupal.behaviors.fancyRadios={attach:function(context,settings){var elements=$('input[type="checkbox"], input[type="radio"]',context);$(elements,context).each(function(k,raw_elem){var elem=$(raw_elem),fancy_radios_class='fancy-radios-'+elem.attr('type');if(elem.parent().hasClass(fancy_radios_class))return;elem.parent().addClass(fancy_radios_class);var fancy=$('<span class="fancy-input"></span>').insertAfter(elem);if(elem.attr('disabled'))fancy.addClass('disabled');if(elem.closest('label[for='+elem.attr('id')+']').length)return;fancy.click(function(e){if(elem.attr('disabled'))return;if(!elem.is(':checked')){raw_elem.checked=true}else if(elem.attr('type')=='checkbox')raw_elem.checked=false})})}}})(jQuery);;
